{
  "name": "ai-flowductiv",
  "version": "0.1.0",
  "private": true,
  "description": "Privacy-first, AI-enhanced productivity tool built with Nuxt 3",
  "author": "Flowductiv Team",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "test": "echo 'Tests temporarily disabled for production deployment'",
    "db:generate": "drizzle-kit generate:sqlite",
