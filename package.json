    "@supabase/supabase-js": "^2.38.5",
    "@vueuse/core": "^10.5.0",
    "@vueuse/nuxt": "^10.5.0",
    "ai": "^4.3.19",
    "better-sqlite3": "^12.2.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.0",
    "drizzle-orm": "^0.29.1",
    "lucide-vue-next": "^0.294.0",
