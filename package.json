    "db:migrate": "bun run server/database/migrate.ts",
    "db:migrate:postgres": "bun run server/database/migrate-postgres.ts",
    "db:studio": "drizzle-kit studio",
    "db:studio:postgres": "drizzle-kit studio --config=drizzle-postgres.config.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.0",
    "@nuxt/devtools": "latest",
