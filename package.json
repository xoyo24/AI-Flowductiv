    "@vite-pwa/nuxt": "^0.4.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/test-utils": "^2.4.6",
    "drizzle-kit": "^0.20.6",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "nuxt": "^3.8.0",
    "playwright-core": "^1.54.1",
    "typescript": "^5.3.0",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
